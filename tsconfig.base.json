{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"strict": true,
		"target": "ES5",
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": false,
		"noFallthroughCasesInSwitch": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitReturns": true,
		"removeComments": true,
		"declaration": true,
		"skipLibCheck": true
	}
}

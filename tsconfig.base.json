{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"strict": true,
		"target": "ES2022",
		"module": "CommonJS",
		"moduleResolution": "Node10",
		"sourceMap": false,
		"noFallthroughCasesInSwitch": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitReturns": true,
		"noEmit": true,
		"skipLibCheck": true
	}
}
